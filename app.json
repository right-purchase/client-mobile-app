{
  "expo": {
    "name": "Compra Certa",
    "slug": "client-mobile-app",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 6,
      "package": "com.rightpurchase.clientmobileapp"
    },
    "web": {
      "favicon": "src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "970da296-98d5-4660-a272-6845b2b3ab1a"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ]
  }
}
